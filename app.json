{
  "expo": {
    "name": "Autovad",
    "slug": "autovad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.autovad.app",
      "supportsTablet": true,
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "package": "com.autovad.app",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "autovad",
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "50a3ddf4-cb9c-4921-b1f5-0134e7e9748e"
      }
    },
    "owner": "mihaimar",
    "updates": {
      "url": "https://u.expo.dev/50a3ddf4-cb9c-4921-b1f5-0134e7e9748e"
    }
  }
}
